{
  "manifest_version": 3,
  "name": "Multi-Search",
  "version": "1.1",
  "description": "Search multiple engines with a custom keyword.",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "ms"
  },
  "action": {
    "default_icon": {
      "48": "icon48.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
